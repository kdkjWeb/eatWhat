/* pages/member/member.wxss */
.memberBox {
  padding:0 15px;
  box-sizing: border-box;
  height:calc(100% - 102px);
}
.member{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
}
.member>view{
  min-width: 20%;
  /* flex: 1; */
  text-align: center;
  margin-top: 10px;
}
.member view image{
  height: 53px;
  width: 53px;
  border-radius: 53px;
}
.member>view view {
  width:55px;
  font-size:12px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}


/* 底部 */
 .footer {
